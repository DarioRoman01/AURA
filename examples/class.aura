clase Stack(data) {
    peek() => (largo(data) == 0) ? nulo : data[-1];

    push(val) => data:agregar(val);

    vacio() => largo(data) == 0;

    popuwu() => (largo(data) == 0) ? nulo : data:pop();

    size() => largo(data);

    vaciar() {
        mientras(largo(data) > 0) {
            data:pop();
        }
    }
}

s := nuevo Stack(lista[2,34,65,23,39])
s.push(45)
escribirF("data: {}", s.data);
escribirF("the size is: {}", s.size())
s.vaciar();
escribir(s.data);
escribir(s.vacio());
